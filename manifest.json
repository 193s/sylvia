{
  "name": "â™¥sylvia",
  "version": "1.1",
  "manifest_version": 2,
  "description": "Simple script which places image in the bottom right.",
  "icons": {
    "128": "images/icon128.png"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*", "webRequest"],
  "web_accessible_resources": ["sylvia.png"],
  "content_scripts": [{
    "all_frames": false,
    "js": ["jquery-2.2.3.min.js", "content.js"],
    "css": ["style.css"],
    "matches": [ "http://*/*", "https://*/*" ]
  }]
}
